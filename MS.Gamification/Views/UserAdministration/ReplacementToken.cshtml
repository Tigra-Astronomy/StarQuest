@model dynamic

@{
    ViewBag.Title = "A new token has been sent";
}

<div class="panel panel-success">
    <div class="panel-heading">
        <h3 class="panel-title">Check your inbox</h3>
    </div>
    <div class="panel-body">
        A replacement verification email has been sent. Please check your inbox for further instructions.
        If you do not receive it within 15 minutes, please contact the site administrators for assistance.
    </div>
</div>